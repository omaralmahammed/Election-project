                        <li class="nav-item">
                            <a href="@Url.Action("UserDebate", "Home")" class="nav-item nav-link">المناظرات</a>
                        </li>

                        <li class="nav-item">
                            <a href="@Url.Action("VotersDetails", "Home")" class="nav-item nav-link">اين تنتخب</a>
                        </li>
                        @{
                            var inTime = Session["inTime"];
                            if (inTime != null && (bool)inTime)
