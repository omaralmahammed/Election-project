    <!-- Template Stylesheet -->
    <link href="~/css/style.css" rel="stylesheet">

    <style>
        .btn-secondary {
            color: #ffff;
            background-color: forestgreen;
            border-color: forestgreen;
        }
       
        .col-lg-3.col-md-6 {
            text-align: right;
        }

        .col-lg-3.col-md-6 .btn-link {
            display: block;
            text-align: right;
            padding-right: 0;
            margin-right: 0;
        }
    </style>
</head>

