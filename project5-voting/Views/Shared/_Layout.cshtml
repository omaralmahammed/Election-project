                        </li>

                        <li class="nav-item">
                            @{
                                var loggedInUser = Session["logedInUser"];
                                if (loggedInUser == null)
                                {
                                    <a href="@Url.Action("firststeplogin", "Login")" class="nav-item nav-link btn ms-3">تسجيل الدخول</a>
                                }
                                else
                                {
                                    <a href="@Url.Action("LogoutUser", "Login")" class="nav-item nav-link ms-3">تسجيل الخروج</a>
                                }
                            }
                        </li>
                    </ul>
                </div>
            </div>
        </div>
