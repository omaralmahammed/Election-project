                    <a class="btn btn-link" href="@Url.Action("Contact", "Contacts")">اتصل بنا</a>
                    <a class="btn btn-link" href="@Url.Action("Service", "Home")">خدماتنا</a>
                    <a class="btn btn-link" href="@Url.Action("ploice", "Home")">الشروط والأحكام</a>
